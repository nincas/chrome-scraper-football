<?php

// Start autoloading
require_once "vendor/autoload.php";

// Load environment variables
__env();
$file = finder('constants.php', dirname(__FILE__));

// Define constants
if (!$file) dlog('File couldnt found.');
require_once $file;

// Start scraper
$scraper = new \Scraper\Kernel\App\Loader;
$scraper->boot();
