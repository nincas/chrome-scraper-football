<?php

// Start autoloading
require_once "vendor/autoload.php";

// Load environment variables
__env();
$file = finder('constants.php', __DIR__);

// Define constants
if (!$file) die('File couldnt found.');
require_once $file;

// Start scraper
$scraper = new \Scraper\Kernel\App\Loader;
$scraper->boot();
